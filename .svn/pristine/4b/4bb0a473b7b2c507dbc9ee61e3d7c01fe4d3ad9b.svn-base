    if (state=="sales_num")
    {
            $('#layer_sales_fund_addup').hide();
            $('#layer_sales_num_addup').show();
            $('#layer_stock_addup').hide();
            $('#layer_fund_addup').hide();
    }
    if (state=="stock_addup")
    {
